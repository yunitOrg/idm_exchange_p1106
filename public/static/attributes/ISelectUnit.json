{
  "classId": "idm.componet.exchange.Iselectunit",
  "comName": "选择单位",
  "className": "ISelectUnit",
  "comType": "common",
  "comLangue": "vue",
  "compositeAttr": [
    {
        "type": "input",
        "layoutType": "inline",
        "text": "唯一标识",
        "bindKey": "ctrlId",
        "disabled": true,
        "default": "@[packageid]",
        "desc": ""
    },
    {
      "type": "group",
      "text": "基本属性",
      "desc": "提供设置组件能达到展示效果的基本属性设置信息",
      "children": [
        {
          "type": "input",
          "layoutType": "inline",
          "text": "内容高度",
          "bindKey": "contentHeight",
          "default": "calc(100% - 204px)"
        }
      ]
    },
    {
      "type": "group",
      "text": "外层样式",
      "desc": "点击？查看样式设置用法指南",
      "helpUrl": "",
      "children": [
          {
            "type": "inlineGroup",
            "text": "宽高",
            "children": [
              {
                "type": "input",
                "layoutType": "block",
                "text": "宽",
                "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                "bindKey": "width",
                "width": "60px",
                "default": "100%"
              },
              {
                "type": "input",
                "layoutType": "block",
                "text": "高",
                "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                "bindKey": "height",
                "width": "60px",
                "default": "100%"
              }
            ]
          },
          {
            "type": "input",
            "layoutType": "block",
            "text": "最大高",
            "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
            "bindKey": "maxHeight"
          },
          {
            "type": "box",
            "layoutType": "block",
            "text": "内外边距",
            "bindKey": "ulbox",
            "default": {
              "marginTopVal": "",
              "marginRightVal": "",
              "marginBottomVal": "",
              "marginLeftVal": "",
              "paddingTopVal": "",
              "paddingRightVal": "18px",
              "paddingBottomVal": "0",
              "paddingLeftVal": "18px"
            }
          },
          {
            "type": "colorPicker",
            "layoutType": "inline",
            "text": "背景色",
            "bindKey": "bgColor",
            "placeholder": "请选择颜色",
            "default": {
                "hex": "#ffffff",
                "hex8": "#ffffff"
            }
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "容器阴影",
            "bindKey": "boxShadow",
            "default": ""
          },
          {
            "type": "border",
            "layoutType": "block",
            "text": "边框",
            "bindKey": "boxborder"
          }
      ]
    },
    {
      "type": "group",
      "text": "高级",
      "desc": "点击？查看高级设置用法指南",
      "children": [
        {
          "type": "actionSelect",
          "layoutType": "block",
          "text": "确定按钮",
          "desc": "确定按钮回调 接口参数：自定义函数参数：{_this: this, item: params}",
          "bindKey": "handleSureBtnFunc",
          "multiple": false
        },
        {
          "type": "actionSelect",
          "layoutType": "block",
          "text": "接口参数",
          "desc": "接口参数：自定义函数参数：{_this: this}",
          "bindKey": "handleParamsFunc",
          "multiple": false
        }
      ]
    }
  ]
}